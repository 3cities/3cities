{
  "name": "@3cities/react-app",
  "version": "1.0.0",
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@3cities/contracts": "^1.0.0",
    "@apollo/client": "^3.5.10",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.4",
    "@usedapp/core": "^1.0.0",
    "graphql": "^16.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "ipfs-deploy": "^11.2.0",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "react-scripts": "https://gitpkg.now.sh/ryanberckmans/create-react-app/packages/react-scripts?postcss-tailwind-fix-v0.1",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-raw": "react-scripts build # run a regular build in the context of an env that's already been set",
    "build:dev": "yarn env-cmd -e dev yarn build-raw # run a production build using dev env, eg. with source maps enabled for use with 'yarn analyze'",
    "build:prod-test": "yarn env-cmd -e prod-test yarn build-raw",
    "build:prod": "npx env-cmd -e prod npm run build-raw",
    "build": "yarn build:prod",
    "eject": "react-scripts eject",
    "ipfs": "yarn build && ipfs-deploy build/",
    "start-raw": "react-scripts start # run dev mode in the context of an env that's already been set",
    "start:dev": "yarn env-cmd -e dev yarn start-raw",
    "start:prod-test": "yarn env-cmd -e prod-test yarn start-raw",
    "start:prod": "yarn env-cmd -e prod yarn start-raw",
    "start": "yarn start:dev",
    "test": "react-scripts test"
  },
  "license": "MIT"
}
